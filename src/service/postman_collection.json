		{
			"name": "Expire Token (Token Already Expired)",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "de6ea6e1-c4e6-4011-bf4d-7ca7b30f3bf9",
						"type": "text/javascript",
						"exec": [
							"pm.sendRequest({",
							"    url: \"http://127.0.0.1:8080/users/authenticate\",",
							"    method: \"POST\",",
							"    header: \"content-type: application/json\",",
							"    body: {",
							"        mode: \"raw\",",
							"        raw: JSON.stringify({",
							"            \"username\": pm.globals.get(\"TEST_USER\"),",
							"            \"password\": pm.globals.get(\"TEST_PASS\")",
							"        })",
							"    }",
							"}, function (err, response) {",
							"    var token = response.json().token;",
							"    pm.globals.set(\"TEST_EXPIRED_TOKEN\", token);",
							"    console.log(\"Authenticated token: \" + token);",
							"});",
							"",
							"pm.sendRequest({",
							"    url: \"http://127.0.0.1:8080/users/expire\",",
							"    method: \"POST\",",
							"    header: \"content-type: application/json\",",
							"    body: {",
							"        mode: \"raw\",",
							"        raw: JSON.stringify({",
							"            \"token\": pm.globals.get(\"TEST_EXPIRED_TOKEN\")",
							"        })",
							"    }",
							"}, function (err, response) {",
							"    console.log(\"Token expired\");",
							"});"
						]
					}
				},
